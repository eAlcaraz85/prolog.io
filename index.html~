<span style="color: #51afef;">(</span><span style="color: #51afef;">defun</span> <span style="color: #5cEfFF;">areaCirculo</span><span style="color: #C57BDB;">(</span>r<span style="color: #C57BDB;">)</span>
<span style="color: #C57BDB;">(</span>* *pi* <span style="color: #7bc275;">(</span>* r r<span style="color: #7bc275;">)</span>  <span style="color: #C57BDB;">)</span>
<span style="color: #51afef;">)</span>

