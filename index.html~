<span style="color: #e69055;">(</span>recorre <span style="color: #a991f1;">(</span>cdr lista<span style="color: #a991f1;">)</span><span style="color: #e69055;">)</span><span style="color: #7bc275;">)</span><span style="color: #C57BDB;">)</span>

