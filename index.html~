                          <span style="color: #FCCE7B;">(</span>Unidad r&#225;pida pero de corto rango de ataque y bajo da&#241;o<span style="color: #FCCE7B;">)</span><span style="color: #a991f1;">)</span>
                         <span style="color: #a991f1;">(</span>Atormentador
                          <span style="color: #FCCE7B;">(</span>Unidad peque&#241;a y de r&#225;pida producci&#243;n<span style="color: #FCCE7B;">)</span><span style="color: #a991f1;">)</span><span style="color: #e69055;">)</span>
                        <span style="color: #7bc275;">)</span>
                       <span style="color: #7bc275;">(</span>Terran
                        <span style="color: #e69055;">(</span>Terrestre
